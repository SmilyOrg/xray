{
	"name": "Haxe",
	"scopeName": "source.haxe",
	"fileTypes": ["hx"],
	"patterns":
	[
		{
			"match": "\".*?\"",
			"name": "string"
		},
		{
			"match": "//.+",
			"name": "comment"
		},
		{
			"match": "\\b(class|typedef|abstract|enum|package|import)\\b",
			"name": "directive"
		},
		{
			"match": "\\b(if|else|try|catch|function|public|private|static|inline|macro|trace)\\b",
			"name": "keyword"
		},
		{
			"match": "\\b(true|false|null)\\b",
			"name": "constant"
		},
		{
			"match": "(\\^|>>>|\\-|>>|<<|\\|\\||\\!=|\\*|%|<=|<|\\.\\.\\.|>=|>|==|/|\\||&|\\+=|-=|/=|\\*=|<<=|>>=|>>>=|\\|=|&=|\\^=|=|=>|&&|\\+)",
			"name": "operator"
		},
		{
			"match": "(\\!|~|\\-|\\+\\+|\\-\\-)",
			"name": "operator"
		}
	]
}